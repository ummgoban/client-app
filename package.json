{
  "name": "ClientApp",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "preinstall": "sh ./.script/pre-install.sh",
    "postinstall": "sh ./.script/post-install.sh",
    "ci": "sh ./.script/ci.sh",
    "android": "ENVFILE=.env react-native run-android",
    "android-release": "ENVFILE=.env.release react-native run-android --mode=release",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "tsc": "tsc",
    "start": "react-native start",
    "test": "jest",
    "format:check": "prettier --check --ignore-path .prettierignore .",
    "format:fix": "prettier --write --ignore-path .prettierignore .",
    "build:android-clean": "cd android && ./gradlew clean",
    "build:android-debug-apk": "cd android && ENVFILE=.env ./gradlew build",
    "build:android-debug-aab": "cd android && ENVFILE=.env ./gradlew bundleDebug",
    "build:android-release-apk": "cd android && ENVFILE=.env.release ./gradlew build",
    "build:android-release-aab": "cd android && ENVFILE=.env.release ./gradlew bundleRelease",
    "build:android-store": "sh ./.script/android-build.sh",
    "build:android-release": "yarn build:android-clean && yarn build:android-release-apk"
  },
  "dependencies": {
    "@emotion/native": "^11.11.0",
